{
    "env": {
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "generator-star-spacing": "off",
        "semi": ["error", "always"],
        "space-before-function-paren": ["error", "never"],
        "comma-dangle": ["error", "always"],
        "quotes": ["error", "single"],
        "object-curly-spacing": ["error", "never"]
    }
}